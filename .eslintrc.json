{
  "env": {
    "node": true,
    "jest": true
  },
  "extends": ["plugin:security/recommended", "plugin:jest/recommended",  "plugin:prettier/recommended"],
  "plugins": ["jest", "security", "prettier"],
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "allowImportExportEverywhere": true
  },
  "rules": {
    "semi": ["warn", "never"],
    "quotes": ["warn", "double"],
    "jest/expect-expect": "off"
  }
}
